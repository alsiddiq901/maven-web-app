pipeline {
    agent any
    
    tools{
        maven "Maven_3.9.11"
    }

    stages {
        stage('Clone') {
            steps {
                git 'https://github.com/alsiddiq901/maven-web-app.git'
            }
        }
         stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
